          "action": "./bin/setup.sh",
          "params": "disable",
          "exitmenu": false,
          "refresh": true,
          "status": false,
          "internal": "Disable RescueTime Log"
        },
        {
          "name": "Clear Log",
          "action": "./bin/setup.sh",
